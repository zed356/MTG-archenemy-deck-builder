{
  "expo": {
    "name": "MTG: Archenemy Deckbuilder",
    "slug": "mtg-archenemy-deckbuilder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/nicol-bolas-icon.jpg",
    "scheme": "mtg-archenemy-deckbuilder",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/IK6STMy2FI-Nt3JI2KHIoHfplcLNXzYNGHS18vDQZZ2gMtiQnE3jF2O4yE-NomjQNw21uOrKPmj4uJXMdJE61w.webp",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ztamoa.mtgarchenemydeckbuilder"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "extra": {
      "eas": {
        "projectId": "4a2288ce-f1dc-4451-bc0a-67d62437b98c"
      }
    }
  }
}
